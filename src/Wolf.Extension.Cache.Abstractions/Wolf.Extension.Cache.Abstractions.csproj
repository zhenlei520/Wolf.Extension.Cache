<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461;net462;net47;net471;net472;net48;netstandard2.0;netstandard2.1;</TargetFrameworks>
    <PackageProjectUrl>https://www.nuget.org/packages/$(AssemblyName)</PackageProjectUrl>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Wolf.DependencyInjection" Version="0.0.1-preview-t003" />
    <PackageReference Include="Wolf.Extensions.Serialize.Json.Abstracts" Version="0.0.1-preview-t004" />
  </ItemGroup>
  
  <ItemGroup>
    <Compile Update="ICacheProvider.Hash.cs">
      <DependentUpon>ICacheProvider.cs</DependentUpon>
    </Compile>
    <Compile Update="ICacheProvider.List.cs">
      <DependentUpon>ICacheProvider.cs</DependentUpon>
    </Compile>
    <Compile Update="ICacheProvider.SortSet.cs">
      <DependentUpon>ICacheProvider.cs</DependentUpon>
    </Compile>
  </ItemGroup>
  
</Project>
